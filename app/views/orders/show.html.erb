<% form.with url: "https://ccore.newebpay.com/MPG/mpg_gateway", method: :post do |form| >
  <% @form_info.each do |key, value|>
    <%= form.hidden_field :MerchantID, { value: @form_info[:MerchantID] } >

    <%= form.text_field :MerchantID , value: @form_info[:MerchantID], class: "hidden" %>
    <%= form.text_field :TradeInfo , value: @form_info[:TradeInfo], class: "hidden"  %>
    <%= form.text_field :TradeSha , value: @form_info[:TradeSha], class: "hidden"  %>
    <%= form.text_field :Version , value: @form_info[:Version], class: "hidden"  %>

  <% end %>
<% end %>